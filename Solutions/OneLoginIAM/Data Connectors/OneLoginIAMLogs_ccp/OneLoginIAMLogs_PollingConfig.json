[
  {
    "name": "OneLoginIAMLogsCCPEventsPolling",
    "apiVersion": "2022-10-01-preview",
    "type": "Microsoft.SecurityInsights/dataConnectors",
    "location": "{{location}}",
    "kind": "RestApiPoller",
    "properties": {
      "connectorDefinitionName": "OneLoginIAMLogsCCPDefinition",
      "dataType": "OneLoginEventsV2_CL",
      "dcrConfig": {
        "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
        "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}",
        "streamName": "Custom-OneLoginEventsV2_CL"
      },
      "auth": {
        "type": "OAuth2",
        "ClientId": "{{clientId}}",
        "ClientSecret": "{{clientSecret}}",
        "tokenEndpoint": "https://np-lab.onelogin.com/auth/oauth2/v2/token",
        "tokenEndpointHeaders": {
          "Accept": "application/json"
        },
        "TokenEndpointQueryParameters": {},
        "grantType": "client_credentials"
      },
      "request": {
        "apiEndpoint": "https://np-lab.onelogin.com/api/1/events",
        "httpMethod": "GET",
        "QueryWindowInMin": 10,
        "queryTimeFormat": "yyyy-MM-ddTHH:mm:ssZ",
        "queryParameters": {
          "until": "{_QueryWindowEndTime}",
          "since": "{_QueryWindowStartTime}"
        },
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "eventsJsonPaths": [
          "$.data"
        ],
        "format": "json"
      },
      "paging": {
        "pagingType": "LinkHeader",
        "linkHeaderTokenJsonPath": "$.pagination.next_link"
      }
    }
  },
  {
    "name": "OneLoginIAMLogsCCPUsersPolling",
    "apiVersion": "2022-10-01-preview",
    "type": "Microsoft.SecurityInsights/dataConnectors",
    "location": "{{location}}",
    "kind": "RestApiPoller",
    "properties": {
      "connectorDefinitionName": "OneLoginIAMLogsCCPDefinition",
      "dataType": "OneLoginUsersV2_CL",
      "dcrConfig": {
        "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
        "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}",
        "streamName": "Custom-OneLoginUsersV2_CL"
      },
      "auth": {
        "type": "OAuth2",
        "ClientId": "{{clientId}}",
        "ClientSecret": "{{clientSecret}}",
        "tokenEndpoint": "https://np-lab.onelogin.com/auth/oauth2/v2/token",
        "tokenEndpointHeaders": {
          "Accept": "application/json"
        },
        "TokenEndpointQueryParameters": {},
        "grantType": "client_credentials"
      },
      "request": {
        "apiEndpoint": "https://np-lab.onelogin.com/api/1/users",
        "httpMethod": "GET",
        "QueryWindowInMin": 10,
        "queryTimeFormat": "yyyy-MM-ddTHH:mm:ssZ",
        "queryParameters": {
          "until": "{_QueryWindowEndTime}",
          "since": "{_QueryWindowStartTime}"
        },
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "eventsJsonPaths": [
          "$.data"
        ],
        "format": "json"
      },
      "paging": {
        "pagingType": "LinkHeader",
        "linkHeaderTokenJsonPath": "$.pagination.next_link"
      }
    }
  }
]