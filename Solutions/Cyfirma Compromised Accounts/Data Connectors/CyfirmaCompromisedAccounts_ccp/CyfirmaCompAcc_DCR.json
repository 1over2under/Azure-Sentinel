            }
        },
        "dataSources": {},
        "destinations": {
            "logAnalytics": [
                {
                    "workspaceResourceId": "[variables('workspaceResourceId')]",
                    "name": "clv2ws1"
                }
            ]
        },
        "dataFlows": [
            {
                "streams": [
                    "Custom-CyfirmaCompromisedAccounts_CL"
                ],
                "destinations": [
                    "clv2ws1"
                ],
                "transformKql": "source\n| extend TimeGenerated = now()\n| project-away cookie_sessions, ['title']\n",
                "outputStream": "Custom-CyfirmaCompromisedAccounts_CL"
            }
        ]
    }
}
