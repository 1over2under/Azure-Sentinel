                    },
                    {
                        "name": "description",
                        "type": "string"
                    },
                    {
                        "name": "source",
                        "type": "string"
                    },
                    {
                        "name": "title",
                        "type": "string"
                    },
                    {
                        "name": "recommendations",
                        "type": "string"
                    },
                    {
                        "name": "safe_flag_comments",
                        "type": "string"
                    },
                    {
