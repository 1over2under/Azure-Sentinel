[
	{
		"name": "AwsNetworkFirewall Alert Logs",
		"apiVersion": "2022-12-01-preview",
		"type": "Microsoft.SecurityInsights/dataConnectors",
		"location": "{{location}}",
		"kind": "AmazonWebServicesS3",
		"properties": {
			"connectorDefinitionName": "AwsNetworkFirewallCcpDefinition",
			"destinationTable": "AWSNetworkFirewallAlert",
			"dataTypes": {
				"logs": {
					"state": "enabled"
				}
			},
			"dcrConfig": {
				"streamName": "SENTINEL_AWS_NETWORK_FIREWALL_ALERT_LOGS",
				"dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
				"dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
			},
			"dataFormat": {
				"Format": "Json",
				"IsCompressed": true,
				"compressType": "Gzip"
			},
			"roleArn": "[[parameters('roleArn')]",
			"sqsUrls": [
				"[[parameters('queueUrl')]"
			]
		}
	},
	{
		"name": "AwsNetworkFirewall Flow Logs",
		"apiVersion": "2022-12-01-preview",
		"type": "Microsoft.SecurityInsights/dataConnectors",
		"location": "{{location}}",
		"kind": "AmazonWebServicesS3",
		"properties": {
			"connectorDefinitionName": "AwsNetworkFirewallCcpDefinition",
			"destinationTable": "AWSNetworkFirewallFlow",
			"dataTypes": {
				"logs": {
					"state": "enabled"
				}
			},
			"dcrConfig": {
				"streamName": "SENTINEL_AWS_NETWORK_FIREWALL_FLOW_LOGS",
				"dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
				"dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
			},
			"dataFormat": {
				"Format": "Json",
				"IsCompressed": true,
				"compressType": "Gzip"
			},
			"roleArn": "[[parameters('roleArn')]",
			"sqsUrls": [
				"[[parameters('queueUrl')]"
			]
		}
	},
	{
		"name": "AwsNetworkFirewall Tls Logs",
		"apiVersion": "2022-12-01-preview",
		"type": "Microsoft.SecurityInsights/dataConnectors",
		"location": "{{location}}",
		"kind": "AmazonWebServicesS3",
		"properties": {
			"connectorDefinitionName": "AwsNetworkFirewallCcpDefinition",
			"destinationTable": "AWSNetworkFirewallTls",
			"dataTypes": {
				"logs": {
					"state": "enabled"
				}
			},
			"dcrConfig": {
				"streamName": "SENTINEL_AWS_NETWORK_FIREWALL_TLS_LOGS",
				"dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
				"dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
			},
			"dataFormat": {
				"Format": "Json",
				"IsCompressed": true,
				"compressType": "Gzip"
			},
			"roleArn": "[[parameters('roleArn')]",
			"sqsUrls": [
				"[[parameters('queueUrl')]"
			]
		}
	}
]