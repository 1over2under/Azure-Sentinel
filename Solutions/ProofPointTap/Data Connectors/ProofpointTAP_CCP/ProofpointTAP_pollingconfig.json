[{
    "name": "ProofpointTAPClicksBlocked",
    "apiVersion": "2022-12-01-preview",
    "type": "Microsoft.SecurityInsights/dataConnectors",
    "location": "{{location}}",
    "kind": "RestApiPoller",
    "properties": {
        "dataType": "ProofPointTAPClicksBlockedV2_CL",
        "response": {
            "eventsJsonPaths": [
                "$.clicksBlocked"
            ],
            "format": "json"
        },
        "connectorDefinitionName": "ProofpointTAPv2",
        "auth": {
            "type": "Basic",
            "UserName": "{{username}}",
            "Password": "{{password}}"
        },
        "request": {
            "apiEndpoint": "https://tap-api-v2.proofpoint.com/v2/siem/clicks/blocked?format=json",
            "rateLimitQPS": 10,
            "queryWindowInMin": 5,
            "httpMethod": "GET",
            "retryCount": 3,
            "timeoutInSeconds": 60,
            "startTimeAttributeName": "sinceTime",
            "queryTimeFormat": "yyyy-MM-ddTHH:mm:ssZ",
            "headers": {
                "Accept": "application/json",
                "Authorization": "[concat('Basic ', base64(concat(parameters('username'),':',parameters('password'))))]"
            }
        },
        "dcrConfig": {
            "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
            "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}",
            "streamName": "Custom-ProofPointTAPClicksBlockedV2_CL"
        },
        "isActive": true
    }
},
